        $(function() {
            $('.ac_datepicker').datepicker({
                showOn: 'button',
                //buttonImage: 'https://dequeuniversity.com/assets/images/calendar.png', // File (and file path) for the calendar image
                //buttonImageOnly: false,
                buttonText: '<span class="ac_datepicker_icon"><i class="icon-append2 fa fa-calendar"></i></span>',
                dayNamesShort: ["星期天", "星期一", "星期二", "星期三", "星期四", "星期五", "星期六"],
                showButtonPanel: true,
                closeText: '關閉視窗',
                dateFormat: "yy.mm.dd",
                //onClose: removeAria,
                // appendText: '<i class="icon-append fa fa-calendar"></i>'
            });

