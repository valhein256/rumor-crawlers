        });

        //matchHeight
        setTimeout(function() {
                $('.cbp-item').matchHeight();
            },
            400);

        //搜尋
        $('button[name="searchSubmit"]').on('click',
            function(e) {
                e.preventDefault();
                var keyword = $('#keyword').val();
                if (!keyword === false) {
                    $('#keyword').val("'" + keyword + "'");
                    console.log($('#keyword').val());
                }
                $('#form').submit();
            });
        $(function () {
            $("*").on('keydown', function (event) {
                if (event.altKey && event.shiftKey && event.which == 83) {
                    //console.log(event)
                    event.preventDefault()
                    $("#keyword").focus()
                }
            });
        })
    </script>

