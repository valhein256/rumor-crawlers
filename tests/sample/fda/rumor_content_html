    <script>
        $(document).ready(function () {
            //$(".slideContent").hide();
            $(".arrow-up").hide();
            $(".slideArrow").click(function () {
                $(this).next(".slideContent").slideToggle(500);
                $(this).find(".arrow-up, .arrow-down").toggle();
            });
        });
    </script>
