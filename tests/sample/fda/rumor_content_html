    <script>
        /*$(function(){
          SyntaxHighlighter.all();
        });*/
        $(window).load(function () {
            //alert($('.bannerList .flexslider .slides li:first').attr('data-duration'));
            $('.bannerList .flexslider').flexslider({
                animation: "slide",
                slideshow: false,                //Boolean: Animate slider automatically
                slideshowSpeed: $('.bannerList .flexslider .slides li:first').attr('data-duration'),           //Integer: Set the speed of the slideshow cycling, in milliseconds
                animationSpeed: 600,            //Integer: Set the speed of animations, in milliseconds
                start: function (slider) {
                    $('body').removeClass('loading');
                    $('.bannerList .flexslider ul.flex-direction-nav a.flex-prev').attr('tabindex', '350');//主要輪播
                    $('.bannerList .flexslider ul.flex-direction-nav a.flex-next').attr('tabindex', '350');
                    $('.bannerList .flexslider .flex-viewport li.clone a').attr('tabindex', '-1');
                    $('.bannerList .flexslider .flex-viewport li:not(.clone) a').on('focus', function () {
                        $('.bannerList .flexslider .flex-viewport li').removeClass("flex-active-slide");
                        $(this).parent('li').addClass("flex-active-slide");
                    });
                },
                after: function (slider) {
                    slider.stop();
                    slider.vars.slideshowSpeed = $(slider.slides[slider.currentSlide]).data('duration');
                    slider.play();
                }
            });
            //活動訊息
            $('.itemList .flexslider').flexslider({
                animation: "slide",
                slideshow: true,                //Boolean: Animate slider automatically
                slideshowSpeed: 7000,           //Integer: Set the speed of the slideshow cycling, in milliseconds
                animationSpeed: 600,            //Integer: Set the speed of animations, in milliseconds
                start: function (slider) {
                    $('body').removeClass('loading');
                    $('.itemList .flexslider ul.flex-direction-nav a.flex-prev').attr('tabindex', '530');//活動訊息
                    $('.itemList .flexslider ul.flex-direction-nav a.flex-next').attr('tabindex', '530');
                    $('.itemList .flexslider .flex-viewport li.clone a').attr('tabindex', '-1');
                    $('.itemList .flexslider .flex-viewport li a').on('focus', function () {
                        $('.itemList .flexslider .flex-viewport li').removeClass("flex-active-slide");
                        $(this).parent('li').addClass("flex-active-slide");
                    });
                },
                after: function (slider) {
                    slider.stop();
                    slider.vars.slideshowSpeed = $(slider.slides[slider.currentSlide]).data('duration');
                    slider.play();
                }
            });
            //影音專區
            $('.itemList1 .flexslider').flexslider({
                animation: "slide",
                slideshow: true,                //Boolean: Animate slider automatically
                slideshowSpeed: 7000,           //Integer: Set the speed of the slideshow cycling, in milliseconds
                animationSpeed: 600,            //Integer: Set the speed of animations, in milliseconds
                start: function (slider) {
                    $('body').removeClass('loading');
                    $('.itemList1 .flexslider ul.flex-direction-nav a.flex-prev').attr('tabindex', '540');//影音專區
                    $('.itemList1 .flexslider ul.flex-direction-nav a.flex-next').attr('tabindex', '540');
                    $('.itemList1 .flexslider .flex-viewport li.clone a').attr('tabindex', '-1');
                    $('.itemList1 .flexslider .flex-viewport li a').on('focus', function () {
                        $('.itemList1 .flexslider .flex-viewport li').removeClass("flex-active-slide");
                        $(this).parent('li').addClass("flex-active-slide");
                    });
                },
                after: function (slider) {
                    slider.stop();
                    slider.vars.slideshowSpeed = $(slider.slides[slider.currentSlide]).data('duration');
                    slider.play();
                }
            });
            //相關連結
            $('.itemList2 .flexslider').flexslider({
                animation: "slide",
                slideshow: true,                //Boolean: Animate slider automatically
                slideshowSpeed: 7000,           //Integer: Set the speed of the slideshow cycling, in milliseconds
                animationSpeed: 600,            //Integer: Set the speed of animations, in milliseconds
                start: function (slider) {
                    $('body').removeClass('loading');
                    $('.itemList2 .flexslider ul.flex-direction-nav a.flex-prev').attr('tabindex', '550');//相關連結
                    $('.itemList2 .flexslider ul.flex-direction-nav a.flex-next').attr('tabindex', '550');
                    $('.itemList2 .flexslider .flex-viewport li.clone a').attr('tabindex', '-1');
                    $('.itemList2 .flexslider .flex-viewport li a').on('focus', function () {
                        $('.itemList2 .flexslider .flex-viewport li').removeClass("flex-active-slide");
                        $(this).parent('li').addClass("flex-active-slide");
                    });
                },
                after: function (slider) {
                    slider.stop();
                    slider.vars.slideshowSpeed = $(slider.slides[slider.currentSlide]).data('duration');
                    slider.play();
                }
            });
        });
    </script>
    
    <!--fixed menu的js-->
    <script>
        $(window).bind('scroll', function () {
            if ($(window).scrollTop() > 50) {
                $('.mainMenu').addClass('fixed');
            } else {
                $('.mainMenu').removeClass('fixed');
            }
        });
    </script>
    <!--回到上方的js-->
    <script>
        $(function () {
            $('#topBtn').click(function () {
                $('html,body').animate({ scrollTop: '0px' }, 600);
                return false;
            });
        });
    </script>
